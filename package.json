{
  "name": "@vue-storefront/odoo-theme",
  "version": "1.0.0-rc.4",
  "private": true,
  "scripts": {
    "build": "nuxt build -m --dotenv .env",
    "test:e2e": "cypress open --config-file tests/e2e/cypress.json",
    "test:e2e:hl": "cypress run --headless --config-file tests/e2e/cypress.json",
    "build:analyze": "nuxt build -a -m",
    "cypress:open": "cypress open",
    "lint": "eslint . --ext .ts,.vue",
    "lint:ts": "tslint --project tsconfig.json --config tslint.json",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "start": "nuxt start",
    "precommit": "lint-staged",
    "update:check": "ncu",
    "update:update": "ncu -u",
    "test": "jest"
  },
  "lint-staged": {
    "*.ts": "yarn lint:ts",
    "*.tsx": "yarn lint:ts"
   },
   "husky": { 
    "hooks": {
       "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/amp": "^0.5.4",
    "@nuxtjs/style-resources": "1.1.0",
    "@storefront-ui/vue": "0.12.0",
    "@vue-storefront/cache": "^2.5.0",
    "@vue-storefront/middleware": "2.5.0",
    "@vue-storefront/nuxt": "2.5.0",
    "@vue-storefront/nuxt-theme": "2.5.0",
    "@vue-storefront/odoo": "1.2.0",
    "cookie-universal-nuxt": "^2.1.3",
    "core-js": "^2.6.5",
    "currency.js": "^2.0.4",
    "eslint": "^8.9.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "nuxt": "2.15.6",
    "nuxt-i18n": "^6.5.0",
    "nuxt-precompress": "^0.5.9",
    "redis-tag-cache": "^1.2.1",
    "vee-validate": "^3.2.3",
    "vue-frag": "^1.4.0",
    "vue-scrollto": "^2.17.1"
  },
  "devDependencies": {
    "@nuxt/image": "^0.6.1",
    "@nuxt/types": "latest",
    "@nuxt/typescript-build": "latest",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/tailwindcss": "^4.1.3",
    "@nuxtjs/web-vitals": "^0.1.8",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "autoprefixer": "^10.2.6",
    "babel-jest": "^24.1.0",
    "cypress": "7.5.0",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.5",
    "tailwindcss": "^2.1.4",
    "vue-cli-plugin-tailwind": "~2.0.6",
    "vue-jest": "^4.0.0-0"
  }
}
